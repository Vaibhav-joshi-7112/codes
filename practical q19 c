#include <stdio.h>


void print_arrtranspose(int arr[][100],int rows,int col){
  for(int i=0;i<rows;i++){
    for(int j=0;j<col;j++){
     printf("%d ",arr[j][i]);
      
}
printf("\n");
   
    }
  }

void initialise_matrix(int arr[][100],int rows,int col){
  for(int i=0;i<rows;i++){
    for(int j=0;j<col;j++){
     scanf("%d",&arr[i][j]);
      
      
    }
   printf("\n");  
  }
  print_arrtranspose(arr,rows,col);
  }


int main(){
  int arr[100][100];
 int rows;scanf("%d",&rows);
  int col;scanf("%d",&col);
  printf("enter value of elements of 2d array\n");
  initialise_matrix(arr,rows,col);
 return 0; 
}  
