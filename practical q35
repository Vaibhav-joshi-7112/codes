#include <stdio.h>

int main() {
    int arr[2501]={0};
    for(int i=2;i<2501;i++){
        arr[i]=1;
    }
    for(int i=3;i<2501;i++){

     
        if(i%2==0){
            arr[i]=0;
    }}
    for(int i=3;i<2501;i++){
        if(arr[i]!=0){
             for(int j=i+1;j<2501;j++){
                 if(j%i==0){
                     arr[j]=0;
                 }
             }
        }
    } FILE *ptr;ptr=fopen("primes.out","w");
    for(int i=1;i<=2500;i++){
        if(arr[i]==1){
       
  for(i=1;i<2501;i++){
     fprintf(ptr,"%d\n",i);
         
  }
        }
    }
    
    return 0;
}
