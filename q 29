#include <stdio.h>
#include <string.h>

int main() {
   
   char st1[100];
   fgets(st1,100,stdin);
   int n;
   n=strlen(st1);
   st1[strlen(st1)-1]='\0';
  char st2[100];
   fgets(st2,100,stdin);
   st2[strlen(st2)-1]='\0';
   int n1=strlen(st2);
    
 int count=0;
   int i=0;
   while(st1[i]!='\0'){
       count=0;
       if(st1[i]==st2[0]){
      for(int j=0, k=i;j<n1;j++,k++){
          if(st1[k]==st2[j]){
              count++;
          }
          
      } 
          if(count==n1){
              
         printf("%d",i);
         return;
        
          
          }}
          i++;
   }printf("-1");
   

}
