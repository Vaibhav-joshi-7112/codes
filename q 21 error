#include <stdio.h>
#include <string.h>

int main() {
   char st[1000];
   fgets(st,1000,stdin);
   char st1[100];
   fgets(st1,100,stdin);
   int n;
   n=strlen(st1);
   // remove newline
   printf("to be replaced with : \n");
  char st2[100];
   fgets(st2,100,stdin);
   
   int n1=strlen(st2);
    
  char st3[1000];
    int count=0;int st3i=0;
   int i=0;
   while(st[i]!=0){
       count=0;
       if(st[i]==st1[0]){
      for(int j=0, k=i;j<n,k<i+n;j++,k++){
          if(st[k]==st1[j]){
              count++;
          }
          
      } }
          if(count==n){
               for(int j=0;j<n1;j++){
          st3[st3i++]=st2[j];
              
          }
          i=i+n1;
          
          }else{
         
      
        st3[st3i++]=st[i];
       i++;
   }}
   st3[st3i]='\0';
   puts(st3);
   
   

    return 0;
}
