#include <stdio.h>
#include<math.h>
int main() {
	float a,b,c;
	scanf("%f",&a);
	scanf("%f",&b);
	scanf("%f",&c);
	float d=(float)b*b-4*a*c;
	if(d>0){
		printf("real and distinct\n");
	float x1=(-b+pow(d,0.5))/(2*a);
	float x2=(-b-pow(d,0.5))/(2*a);
	printf("x1 : %f\n x2 : %f",x1,x2 );
	}
	else if(d==0){
		float x1=(-b)/(2*a);
	printf("Real and Equal");
			printf("x1 : %f\n x2 : %f",x1,x1 );
	}
	else{
		printf("Imaginary");
	}


	return 0;
}
